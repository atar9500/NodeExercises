{
  "presets": ["@babel/preset-env", "@babel/preset-typescript"],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    [
      "module-resolver",
      {
        "root": ["./src"],
        "extensions": [".js", ".ts", ".tsx", ".json"],
        "alias": {
          "test": "./test",
          "~": "./src"
        }
      }
    ]
  ]
}
